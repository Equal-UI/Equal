{
  "name": "equal-vue",
  "version": "0.42.0",
  "description": "Equal is a Vue UI library based on TypeScript",
  "author": {
    "name": "Yan Savinov",
    "email": "yan.savinov.hire@gmail.com"
  },
  "private": false,
  "license": "MIT",
  "bugs": "https://github.com/quatrochan/Equal/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/quatrochan/Equal"
  },
  "keywords": [
    "equal",
    "vue",
    "vue2",
    "vuejs",
    "typescript",
    "vue-components",
    "ui",
    "framework"
  ],
  "main": "./dist/equal.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib src/index.ts --name equal",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "docs:dev": "nuxt ./docs",
    "docs:generate": "DEPLOY_ENV=GH_PAGES nuxt generate ./docs"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-focus-lock": "^1.4.0",
    "vue-property-decorator": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@nuxt/typescript-build": "^0.6.1",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-typescript": "^3.10.0",
    "@vue/cli-plugin-unit-jest": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "closure-webpack-plugin": "^2.3.0",
    "google-closure-compiler": "^20200224.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.2.6",
    "nuxt": "^2.9.2",
    "nuxt-class-component": "^1.3.0",
    "prismjs": "^1.17.1",
    "push-dir": "^0.4.1",
    "ts-jest": "^23.0.0",
    "typescript": "^3.7.3",
    "vue-prism-component": "^1.1.1",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
